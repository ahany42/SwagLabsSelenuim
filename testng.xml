<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SwagLabs Test Suite" verbose="1" parallel="false">

    <listeners>
        <listener class-name="TestListener"/>
    </listeners>

    <test name="Login to Purchase">
        <classes>
            <class name="LoginTestCase"/>
            <class name="ProductsTestCase"/>
            <class name="PurchaseTestCase"/>
        </classes>
    </test>
</suite>
